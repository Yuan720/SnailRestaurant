<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.woniuxy.snailrestaurant.mapper.WalletMapper">

    <resultMap id="BaseResultMap" type="com.woniuxy.snailrestaurant.domain.Wallet">
            <id property="id" column="id" jdbcType="INTEGER"/>
            <result property="userId" column="user_id" jdbcType="INTEGER"/>
            <result property="balance" column="balance" jdbcType="DECIMAL"/>
            <result property="status" column="status" jdbcType="TINYINT"/>
            <result property="totalConsum" column="total_consum" jdbcType="DECIMAL"/>
            <result property="use_times" column="use_times" jdbcType="INTEGER" />
    </resultMap>

    <sql id="Base_Column_List">
        id,user_id,balance,
        status
    </sql>
</mapper>
